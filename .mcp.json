{
  "mcpServers": {
    "fear-greed-index": {
      "command": "uv",
      "args": ["run", "python", "fgi_mcp_server.py"],
      "cwd": "/home/jjoravet/autotrade/fear-greed-index",
      "env": {
        "PATH": "${PATH}"
      }
    },
    "alpaca": {
      "command": "uvx",
      "args": ["alpaca-mcp-server", "serve"],
      "env": {
        "ALPACA_API_KEY": "${APCA_API_KEY_ID}",
        "ALPACA_SECRET_KEY": "${APCA_API_SECRET_KEY}"
      }
    }
  }
}
